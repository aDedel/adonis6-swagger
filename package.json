{
  "name": "@dedel.alex/adonis6-swagger",
  "description": "Swagger provider for AdonisJS 6",
  "version": "0.0.13",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js",
    "./commands/*": "./build/commands/*.js",
    "./providers/*": "./build/providers/*.js",
    "./types": "./build/src/types.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "postcompile": "npm run copy:templates",
    "copy:templates": "copyfiles --up 1 \"stubs/**/*.stub\" build",
    "build": "npm run compile",
    "postbuild": "npm run index:commands",
    "release": "np --no-tests",
    "format": "prettier --write .",
    "lint": "eslint . --ext=.ts",
    "version": "npm run build",
    "index:commands": "adonis-kit index build/commands"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@types/node": "^22.7.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-dist": "^3.30.5",
    "copyfiles": "^2.4.1",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "mime": "^4.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.17.14"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.14.1"
  },
  "author": "Dedel Alex",
  "license": "MIT",
  "homepage": "https://github.com/aDedel/adonis6-swagger#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aDedel/adonis6-swagger.git"
  },
  "bugs": {
    "url": "https://github.com/aDedel/adonis6-swagger/issues"
  },
  "keywords": [
    "adonis",
    "adonis6",
    "adonisjs",
    "adonisjs6",
    "swagger",
    "typescript"
  ],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "adonisjs": {
    "types": "adonis6-swagger",
    "providers": [
      "adonis6-swagger"
    ],
    "commands": [
      "adonis6-swagger/build/commands"
    ]
  },
  "np": {
    "message": "chore(release): %s",
    "contents": ".",
    "anyBranch": false
  }
}
